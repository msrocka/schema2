{
  "$id": "DQIndicator.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "title": "DQIndicator",
  "properties": {
    "name": {
      "type": "string",
      "protoIndex": 1
    },
    "position": {
      "type": "integer",
      "protoIndex": 2
    },
    "scores": {
      "type": "array",
      "item": {
        "$ref": "DQScore.schema.json"
      },
      "protoIndex": 3
    }
  },
  "description": "An indicator of a data quality system ([DQSystem])."
}